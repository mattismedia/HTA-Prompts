        function openPopup(sMessage) {
            var oPopup = window.createPopup();
            var oPopBody = oPopup.document.body;

            oPopBody.style.backgroundColor = "lightyellow";
            oPopBody.style.border = "solid black 1px";
            // The following HTML that populates the popup object with a string.
            oPopBody.innerHTML = sMessage;
            oPopup.show(10, 10, 480, 25, document.body);
        }

        function onExitKeyDown() {
            if (event.keyCode == 13)
                self.close();
        }

        function onExitFocus() {
            if (event.altKey)
                self.close();
        }

        function onLinkFocus(oLink) {
            if (event.altKey)
