        function onClickLink64App(sApp) {
            var oShell = new ActiveXObject("WScript.Shell");
            var wshSysEnv = oShell.Environment("SYSTEM");

            if (wshSysEnv("PROCESSOR_ARCHITECTURE") == "AMD64") {
                try {
                    var commandLine = oHTA.commandLine;
                    var oShell = new ActiveXObject("WScript.Shell");
                    var sCommandLine = commandLine.toUpperCase();

                    oShell.CurrentDirectory = sCommandLine.substr(1, sCommandLine.indexOf("\\SMSSETUP") - 1);

                    oShell.Run(sApp);
                }
                catch (e) {
                    //=== catch errors ===//
                    if (e.number == -2147024894) {
                        //=== file not found ===//
                        window.alert(L_MESSAGE_FILE_NOT_FOUND_VAR_TEXT + '"' + sApp + '"');
                    }
                    else if (e.number != -2147023673) {
                        //=== unknown error ===//
                        window.alert(L_MESSAGE_UNKNOWN_ERROR_VAR_TEXT + '"' + sApp + '"' + L_MESSAGE_ERROR_NUMBER_VAR_TEXT + e.number);
                    }
                }
            }
            else {
                window.alert(L_MESSAGE_UNSUPPORTED_OS_VAR_TEXT);
            }

        }

