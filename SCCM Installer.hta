                    //=== unknown error ===//
                    window.alert(L_MESSAGE_UNKNOWN_ERROR_VAR_TEXT + '"explorer.exe"' + L_MESSAGE_ERROR_NUMBER_VAR_TEXT + e.number);
                }
            }
        }

